   15 parameter: "identifier" ":=" integer_or_decimal

   16 body: command_block

   17 command_block: '[' command_sequence ']'

   18 command_sequence: ε
   19                 | simple_command ';'
   20                 | command_sequence ';' simple_command ';'

   21 simple_command: variable_declaration_with_instantiation
   22               | function_call
   23               | attribution_command
   24               | return_command
   25               | flow_control_command

   26 variable_declaration: "variable" "identifier" ":=" var_type

   27 variable_declaration_with_instantiation: "variable" "identifier" ":=" var_type optional_instantiation

   28 var_type: "decimal"
   29         | "integer"

   30 optional_instantiation: ',' literal
   31                       | ε

   32 literal: "integer literal"
   33        | "decimal literal"

   34 attribution_command: "identifier" ":=" expression

   35 function_call: "identifier" '(' args ')'

   36 args: ε
   37     | args ',' expression
   38     | expression

   39 return_command: "return" expression ":=" var_type

   40 flow_control_command: conditional_struct
   41                     | iterative_struct

   42 conditional_struct: "if" '(' expression ')' '[' mandatory_block ']' else_block

   43 else_block: ε
   44           | "else" command_block

   45 mandatory_block: mandatory_block simple_command
   46                | simple_command

   47 iterative_struct: "while" '(' expression ')' command_block

   48 expression: "decimal"


